                      <Parameter AllowDbNull="true" AutogeneratedName="SDate" ColumnName="DOC_DATE_GRE" DataSourceName="AIN_INVENTORY.dbo.INV_STK_TRX_HDR" DataTypeServer="datetime" DbType="DateTime" Direction="Input" ParameterName="@SDate" Precision="0" ProviderType="DateTime" Scale="0" Size="8" SourceColumn="DOC_DATE_GRE" SourceColumnNullMapping="false" SourceVersion="Current" />
                      <Parameter AllowDbNull="true" AutogeneratedName="EDate" ColumnName="DOC_DATE_GRE" DataSourceName="AIN_INVENTORY.dbo.INV_STK_TRX_HDR" DataTypeServer="datetime" DbType="DateTime" Direction="Input" ParameterName="@EDate" Precision="0" ProviderType="DateTime" Scale="0" Size="8" SourceColumn="DOC_DATE_GRE" SourceColumnNullMapping="false" SourceVersion="Current" />
