                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        INV_ITEM_DIRECTORY.CODE, INV_ITEM_DIRECTORY.DESC_ENG, SUM(INV_STK_TRX_DTL.QUANTITY) AS Expr1, INV_STK_TRX_DTL.UOM, 
                         SYS_DOC_TYPE.DESC_ENG AS Expr2, INV_STK_TRX_HDR.DOC_DATE_GRE
FROM            INV_ITEM_DIRECTORY INNER JOIN
                         INV_ITEM_DIRECTORY_UNITS ON INV_ITEM_DIRECTORY.CODE = INV_ITEM_DIRECTORY_UNITS.ITEM_CODE INNER JOIN
                         INV_STK_TRX_DTL ON INV_ITEM_DIRECTORY.CODE = INV_STK_TRX_DTL.ITEM_CODE AND 
                         INV_ITEM_DIRECTORY_UNITS.UNIT_CODE = INV_STK_TRX_DTL.UOM INNER JOIN
                         INV_STK_TRX_HDR ON INV_STK_TRX_DTL.DOC_NO = INV_STK_TRX_HDR.DOC_NO INNER JOIN
                         SYS_DOC_TYPE ON INV_STK_TRX_DTL.DOC_TYPE = SYS_DOC_TYPE.CODE
WHERE        (INV_STK_TRX_DTL.DOC_TYPE = 'INV.STK.ADJ' OR
                         INV_STK_TRX_DTL.DOC_TYPE = N'INV.STK.IN' OR
                         INV_STK_TRX_DTL.DOC_TYPE = N'INV.STK.OUT') AND (INV_ITEM_DIRECTORY.CODE LIKE '%' + @ItemCode + '%') AND (INV_STK_TRX_HDR.DOC_DATE_GRE 
                         &gt;= @SDate) AND (INV_STK_TRX_HDR.DOC_DATE_GRE &lt;= @EDate)
GROUP BY INV_ITEM_DIRECTORY.CODE, INV_ITEM_DIRECTORY.DESC_ENG, INV_STK_TRX_DTL.UOM, SYS_DOC_TYPE.DESC_ENG, INV_STK_TRX_HDR.DOC_DATE_GRE</CommandText>
