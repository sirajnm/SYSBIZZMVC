                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        Docno, Invoice, CONVERT(varchar(10), date, 103) AS PurchaseDate, ITEM_CODE, ITEM_DESC_ENG, TypeName, GroupName, CategoryName, TMName, UnitName, 
                         TaxTotal * - 1 AS TaxTotal, Documentname, CONVERT(FLOAT, Quantity * - 1) AS Quantity, CONVERT(FLOAT, GrossTotal * - 1) AS GrossTotal, Supplier, ITEM_TAX
FROM            (SELECT        INV_PURCHASE_HDR.DOC_NO AS Docno, INV_PURCHASE_HDR.DOC_ID AS Invoice, INV_PURCHASE_HDR.DOC_DATE_GRE AS date, 
                                                    INV_PURCHASE_DTL.ITEM_CODE, ISNULL(PAY_SUPPLIER.CODE, N'') AS Suppliercode, SYS_DOC_TYPE.DESC_ENG AS Documentname, 
                                                    INV_PURCHASE_HDR.DOC_TYPE, INV_PURCHASE_DTL.ITEM_DESC_ENG, INV_ITEM_DIRECTORY.[GROUP], 
                                                    INV_ITEM_GROUP.DESC_ENG AS GroupName, INV_ITEM_DIRECTORY.TYPE, INV_ITEM_TYPE.DESC_ENG AS TypeName, 
                                                    INV_ITEM_DIRECTORY.CATEGORY, INV_ITEM_CATEGORY.DESC_ENG AS CategoryName, INV_ITEM_DIRECTORY.TRADEMARK, 
                                                    INV_ITEM_TM.DESC_ENG AS TMName, INV_PURCHASE_DTL.UOM, INV_UNIT.DESC_ENG AS UnitName, SUM(INV_PURCHASE_DTL.QTY_RCVD) 
                                                    AS Quantity, SUM(INV_PURCHASE_DTL.ITEM_GROSS) AS GrossTotal, INV_PURCHASE_HDR.TAX_TOTAL AS TaxTotal, 
                                                    PAY_SUPPLIER.DESC_ENG AS Supplier, INV_PURCHASE_DTL.ITEM_TAX
                          FROM            INV_PURCHASE_HDR LEFT OUTER JOIN
                                                    INV_PURCHASE_DTL ON INV_PURCHASE_HDR.DOC_NO = INV_PURCHASE_DTL.DOC_NO LEFT OUTER JOIN
                                                    INV_ITEM_DIRECTORY ON INV_PURCHASE_DTL.ITEM_CODE = INV_ITEM_DIRECTORY.CODE LEFT OUTER JOIN
                                                    PAY_SUPPLIER ON INV_PURCHASE_HDR.SUPPLIER_CODE = PAY_SUPPLIER.CODE LEFT OUTER JOIN
                                                    SYS_DOC_TYPE ON INV_PURCHASE_HDR.DOC_TYPE = SYS_DOC_TYPE.CODE LEFT OUTER JOIN
                                                    INV_ITEM_CATEGORY ON INV_ITEM_CATEGORY.CODE = INV_ITEM_DIRECTORY.CATEGORY LEFT OUTER JOIN
                                                    INV_ITEM_GROUP ON INV_ITEM_DIRECTORY.[GROUP] = INV_ITEM_GROUP.CODE LEFT OUTER JOIN
                                                    INV_ITEM_TYPE ON INV_ITEM_DIRECTORY.TYPE = INV_ITEM_TYPE.CODE LEFT OUTER JOIN
                                                    INV_ITEM_TM ON INV_ITEM_DIRECTORY.TRADEMARK = INV_ITEM_TM.CODE LEFT OUTER JOIN
                                                    INV_UNIT ON INV_PURCHASE_DTL.UOM = INV_UNIT.CODE
                          WHERE        (INV_ITEM_DIRECTORY.[GROUP] LIKE '%' + @group + '%') AND (INV_ITEM_DIRECTORY.TYPE LIKE '%' + @type + '%') AND 
                                                    (INV_ITEM_DIRECTORY.CATEGORY LIKE '%' + @category + '%') AND (INV_ITEM_DIRECTORY.TRADEMARK LIKE '%' + @trademark + '%') AND 
                                                    (INV_PURCHASE_HDR.DOC_TYPE = 'LGR.PRT') AND (INV_PURCHASE_DTL.FLAGDEL = 'True') AND (INV_PURCHASE_HDR.FLAGDEL = 'True') AND 
                                                    (INV_PURCHASE_HDR.PUR_TYPE LIKE '%' + @BillType + '%') AND (INV_PURCHASE_DTL.ITEM_DESC_ENG LIKE '%' + @item + '%')
                          GROUP BY INV_PURCHASE_DTL.ITEM_CODE, PAY_SUPPLIER.CODE, SYS_DOC_TYPE.DESC_ENG, INV_PURCHASE_HDR.DOC_TYPE, 
                                                    INV_PURCHASE_HDR.TAX_TOTAL, INV_PURCHASE_DTL.ITEM_DESC_ENG, INV_ITEM_DIRECTORY.[GROUP], INV_ITEM_GROUP.DESC_ENG, 
                                                    INV_PURCHASE_HDR.DOC_ID, INV_ITEM_DIRECTORY.TYPE, INV_ITEM_TYPE.DESC_ENG, INV_ITEM_DIRECTORY.CATEGORY, 
                                                    INV_ITEM_CATEGORY.DESC_ENG, INV_ITEM_DIRECTORY.TRADEMARK, INV_ITEM_TM.DESC_ENG, INV_PURCHASE_DTL.UOM, INV_UNIT.DESC_ENG, 
                                                    INV_PURCHASE_HDR.DOC_NO, INV_PURCHASE_HDR.DOC_DATE_GRE, PAY_SUPPLIER.DESC_ENG, INV_PURCHASE_DTL.ITEM_TAX) 
                         AS Purchase
UNION ALL
SELECT        Docno, Invoice, CONVERT(varchar(10), date, 103) AS PurchaseDate, ITEM_CODE, ITEM_DESC_ENG, TypeName, GroupName, CategoryName, TMName, UnitName, 
                         TaxTotal, Documentname, CONVERT(FLOAT, Quantity) AS Quantity, CONVERT(FLOAT, GrossTotal) AS GrossTotal, Supplier, ITEM_TAX
FROM            (SELECT        INV_PURCHASE_HDR_1.DOC_NO AS Docno, INV_PURCHASE_HDR_1.DOC_ID AS Invoice, INV_PURCHASE_HDR_1.DOC_DATE_GRE AS date, 
                                                    INV_PURCHASE_DTL_1.ITEM_CODE, ISNULL(PAY_SUPPLIER_1.CODE, N'') AS Suppliercode, SYS_DOC_TYPE_1.DESC_ENG AS Documentname, 
                                                    INV_PURCHASE_HDR_1.DOC_TYPE, INV_PURCHASE_DTL_1.ITEM_DESC_ENG, INV_ITEM_DIRECTORY_1.[GROUP], 
                                                    INV_ITEM_GROUP_1.DESC_ENG AS GroupName, INV_ITEM_DIRECTORY_1.TYPE, INV_ITEM_TYPE_1.DESC_ENG AS TypeName, 
                                                    INV_ITEM_DIRECTORY_1.CATEGORY, INV_ITEM_CATEGORY_1.DESC_ENG AS CategoryName, INV_ITEM_DIRECTORY_1.TRADEMARK, 
                                                    INV_ITEM_TM_1.DESC_ENG AS TMName, INV_PURCHASE_DTL_1.UOM, INV_UNIT_1.DESC_ENG AS UnitName, 
                                                    SUM(INV_PURCHASE_DTL_1.QTY_RCVD) AS Quantity, SUM(INV_PURCHASE_DTL_1.ITEM_GROSS) AS GrossTotal, 
                                                    INV_PURCHASE_HDR_1.TAX_TOTAL AS TaxTotal, PAY_SUPPLIER_1.DESC_ENG AS Supplier, INV_PURCHASE_DTL_1.ITEM_TAX
                          FROM            INV_PURCHASE_HDR AS INV_PURCHASE_HDR_1 LEFT OUTER JOIN
                                                    INV_PURCHASE_DTL AS INV_PURCHASE_DTL_1 ON INV_PURCHASE_HDR_1.DOC_NO = INV_PURCHASE_DTL_1.DOC_NO LEFT OUTER JOIN
                                                    INV_ITEM_DIRECTORY AS INV_ITEM_DIRECTORY_1 ON INV_PURCHASE_DTL_1.ITEM_CODE = INV_ITEM_DIRECTORY_1.CODE LEFT OUTER JOIN
                                                    PAY_SUPPLIER AS PAY_SUPPLIER_1 ON INV_PURCHASE_HDR_1.SUPPLIER_CODE = PAY_SUPPLIER_1.CODE LEFT OUTER JOIN
                                                    SYS_DOC_TYPE AS SYS_DOC_TYPE_1 ON INV_PURCHASE_HDR_1.DOC_TYPE = SYS_DOC_TYPE_1.CODE LEFT OUTER JOIN
                                                    INV_ITEM_CATEGORY AS INV_ITEM_CATEGORY_1 ON INV_ITEM_CATEGORY_1.CODE = INV_ITEM_DIRECTORY_1.CATEGORY LEFT OUTER JOIN
                                                    INV_ITEM_GROUP AS INV_ITEM_GROUP_1 ON INV_ITEM_DIRECTORY_1.[GROUP] = INV_ITEM_GROUP_1.CODE LEFT OUTER JOIN
                                                    INV_ITEM_TYPE AS INV_ITEM_TYPE_1 ON INV_ITEM_DIRECTORY_1.TYPE = INV_ITEM_TYPE_1.CODE LEFT OUTER JOIN
                                                    INV_ITEM_TM AS INV_ITEM_TM_1 ON INV_ITEM_DIRECTORY_1.TRADEMARK = INV_ITEM_TM_1.CODE LEFT OUTER JOIN
                                                    INV_UNIT AS INV_UNIT_1 ON INV_PURCHASE_DTL_1.UOM = INV_UNIT_1.CODE
                          WHERE        (INV_PURCHASE_HDR_1.DOC_TYPE LIKE '%' + @doctype + '%') AND (INV_ITEM_DIRECTORY_1.[GROUP] LIKE '%' + @group + '%') AND 
                                                    (INV_ITEM_DIRECTORY_1.TYPE LIKE '%' + @type + '%') AND (INV_ITEM_DIRECTORY_1.CATEGORY LIKE '%' + @category + '%') AND 
                                                    (INV_ITEM_DIRECTORY_1.TRADEMARK LIKE '%' + @trademark + '%') AND (INV_PURCHASE_HDR_1.DOC_TYPE IN ('PUR.CRD', 'PUR.CSS')) AND 
                                                    (INV_PURCHASE_DTL_1.FLAGDEL = 'True') AND (INV_PURCHASE_HDR_1.FLAGDEL = 'True') AND 
                                                    (INV_PURCHASE_HDR_1.PUR_TYPE LIKE '%' + @BillType + '%') AND (INV_PURCHASE_DTL_1.ITEM_DESC_ENG LIKE '%' + @item + '%')
                          GROUP BY INV_PURCHASE_DTL_1.ITEM_CODE, PAY_SUPPLIER_1.CODE, SYS_DOC_TYPE_1.DESC_ENG, INV_PURCHASE_HDR_1.DOC_TYPE, 
                                                    INV_PURCHASE_HDR_1.TAX_TOTAL, INV_PURCHASE_DTL_1.ITEM_DESC_ENG, INV_ITEM_DIRECTORY_1.[GROUP], INV_ITEM_GROUP_1.DESC_ENG, 
                                                    INV_PURCHASE_HDR_1.DOC_ID, INV_ITEM_DIRECTORY_1.TYPE, INV_ITEM_TYPE_1.DESC_ENG, INV_ITEM_DIRECTORY_1.CATEGORY, 
                                                    INV_ITEM_CATEGORY_1.DESC_ENG, INV_ITEM_DIRECTORY_1.TRADEMARK, INV_ITEM_TM_1.DESC_ENG, INV_PURCHASE_DTL_1.UOM, 
                                                    INV_UNIT_1.DESC_ENG, INV_PURCHASE_HDR_1.DOC_NO, INV_PURCHASE_HDR_1.DOC_DATE_GRE, PAY_SUPPLIER_1.DESC_ENG, 
                                                    INV_PURCHASE_DTL_1.ITEM_TAX) AS Purchase_1</CommandText>
